<%= form_with model: [:admin, @post] do |form| %>
  <%= form.label :title %><br />
  <%= form.text_field :title %>
  <%= form.label :summary %><br />
  <%= form.rich_text_area :summary %>
  <%= form.label :youtube_video_id %>
  <%= form.text_field :youtube_video_id %>
  <%= form.label :body %><br />
  <%= form.rich_text_area :body %>
  <%= form.submit %>
<% end %>